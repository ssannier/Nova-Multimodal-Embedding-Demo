{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowManifestAndReportAccess",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::cic-multimedia-test/batch-job-manifests/*",
                "arn:aws:s3:::cic-multimedia-test/batch-job-reports/*"
            ]
        },
        {
            "Sid": "AllowLambdaInvoke",
            "Effect": "Allow",
            "Action": "lambda:InvokeFunction",
            "Resource": "arn:aws:lambda:us-east-1:756493389182:function:NovaEmbeddingProcessor"
        }
    ]
}